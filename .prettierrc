{
    "overrides": [
    {
      "files": ["*.html", "*.ts", "*.tsx", "*.js", "*.jsx", "*.json"],
      "options": {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "explicitTypes": "always"
      }
    }
  ]
}
